   "source": [
    "def fib(n):\n",
    "    if n == 0 or n == 1:\n",
    "        return n\n",
    "    else:\n",
    "        return fib(n - 1) + fib(n - 2)\n",
    "    \n",
    "def main():\n",
    "    i = 0\n",
    "    while True:\n",
    "        fib_num = fib(i)\n",
    "        if fib_num > 50:\n",
    "            break\n",
    "        print(fib_num)\n",
    "        i += 1\n",
    "\n",
    "main()\n"
   ]
